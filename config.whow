icons yes
cachedir "/path/to/cache"
timeformat 12

date {
    format "%a, %b %d %Y"
    literal "mm/dd/yyyy"
}

category [
    "art" "programming:#ff00ff" "asap:$RED" "fun:$GREEN"
]

format [
    "separator:1" "date" "separator:1" "calendar"
    "separator:2" "important" "todos" "separator:2"
    "events" "separator:2" "schedule" "customtask"
]

custom {
    customtask "shell command"
}
